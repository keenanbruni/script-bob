{
  "name": "ScriptBob",
  "version": "1.0.0",
  "description": "LLM-powered HTML editor Chrome DevTools extension",
  "manifest_version": 3,
  "devtools_page": "devtools.html",
  "permissions": ["scripting", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "ScriptBob"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}